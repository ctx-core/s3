{
	"name": "@ctx-core/s3",
	"version": "11.0.45",
	"description": "ctx-core s3",
	"keywords": [
		"ctx-core",
		"sass"
	],
	"homepage": "https://github.com/ctx-core/s3#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/s3/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/s3/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/env": "workspace:^14.0.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
