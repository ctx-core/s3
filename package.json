{
	"name": "@ctx-core/s3",
	"version": "13.2.179",
	"description": "ctx-core s3",
	"keywords": [
		"ctx-core",
		"sass"
	],
	"homepage": "https://github.com/ctx-core/s3#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/s3/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/s3.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"getObject",
		"getObject_maybe",
		"LastModified_date",
		"listObjects",
		"putObject",
		"s3_client",
		"s3_env__validate"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.529.1",
		"ctx-core": "^6.5.4"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}